			workTool += "|0x3CF800 .";
			if( j < tools.length() - 1 ) {
				workTool = substring( workTool, 0, workTool.length() - 2 ) + "|0x3CF800 , или:\n";
			}
		}
		
		cr.Say( SAY_NETMSG, workTool );
		
