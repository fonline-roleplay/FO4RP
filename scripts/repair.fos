			part += "|0x3CF800  - ";
			part += "|0xFFFF00 " + parts[jj + 1];
			part += "|0x3CF800  шт.,\n";
		}
		
		part[ part.length() - 2 ] = ' ';
		part[ part.length() - 1 ] = ' ';
		cr.Say( SAY_NETMSG, part );
		return true;
	}
	
	if( item.GetType() == ITEM_TYPE_WEAPON && item.Proto.Weapon_MaxAmmoCount > 0 ) {
		uint ammoCount = item.AmmoCount;
		
