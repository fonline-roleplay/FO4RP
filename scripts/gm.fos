void setItemFlags( Critter& player, uint n )
{
    Item@ item = GetItem( player.Stat[ ST_VAR0 ] );
    if( @item is null ) return;
	if( FLAG( item.Flags, ItemFlags[n] ) )
		UNSETFLAG( item.Flags, ItemFlags[n] );
	else SETFLAG( item.Flags, ItemFlags[n] );
}

