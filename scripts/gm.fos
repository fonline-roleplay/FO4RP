class Poll
{
    uint   Id;
    uint   InitiatorId;
    string Question;
    uint8  AnsNum;
    string[] Answers;
    uint16[] Votes;

    Poll()
    {
        Id = 0;
        InitiatorId = 0;
        AnsNum = 0;
    }

    Poll@ Set( uint id, uint initiatorId, string& question )
    {
        Id = id;
        InitiatorId = initiatorId;
        Question = question;
        return this;
    }
    Poll@ Add( string& answer )
    {
        AnsNum++;
        Answers.insertLast( answer );
        Votes.insertLast( 0 );
        return this;
    }
    void Vote( uint num )
    {
        if( num >= AnsNum )
            return;
        Votes[ num ] += 1;
    }
}

Poll@[] polls;
uint poll_lastId = 0;

void unsafe_poll( Critter& player, int param0, int param1, int param2, string@ param3, int[] @ param4 )
