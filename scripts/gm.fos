        {
            f.writeString( "" + date + "\t" + player.Id + "\t" + type + "\t" + GetPlayerName( player.Id ) + "\t" + "\t" + " opened lock " + lock + " with lock difficulty of " + compl +" at " + "\t" + place + "\n" );
