    if( @gm != null )
    {
        gm.Say( SAY_NETMSG, "Player " + GetPlayerName( cr.Id ) + "(" + cr.Id + ") chose variant " + answerI );
    }
    cr.ParamBase[ ST_VAR3 ] = -1;
}

