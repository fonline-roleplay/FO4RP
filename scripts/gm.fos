void give( Critter& player, int crId, int itemId, int p2 )
{
    Critter@ cr = GetCritter( uint( crId ) );
    if( cr is null )
        return;

    Item@ item = GetItem( itemId );
    if( item is null )
        return;

    uint count = 1;
    if( item.IsStackable() )
    {
        count = item.GetCount();
        if( p2 > 0 && p2 < int( count ) )
            count = p2;
    }
    MoveItem( item, count, cr );
}

