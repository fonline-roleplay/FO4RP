	string[] GAMEMODE_INFO = { "Start", "Adventurer", "Survivor", "Test" };
	if( cr.GetAccess() > ACCESS_MODER )
	{
		result += " Admin";
	}
	
	if( isGM( cr ) )
	{
		result += " GM";
	}
	
	int game_mode = cr.Param[ QST_GAMEMODE ];
	if( game_mode < GAME_START || game_mode > GAME_TEST )
