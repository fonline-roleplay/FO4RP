void _ShowPlayer(Critter&npc,Critter&player)
{
	Critter@target=::GetCritter(player.Id);
	if(not player.IsPlayer())
	return;
	
	if(npc.StatBase[(112)]==player.StatBase[(112)])
	return;
	
	if(npc.StatBase[(98)]>npc.StatBase[(99)])
	{
		
		if(npc.IsNoPlanes())
		AddPlaneEscape(npc,player);
	}
	else
	{
		if(player.Param[(703)]!=(3))
		npc.SendMessage((16001),target.Id,(0));
		AddAttackPlane(npc,10,player);
	}
}

