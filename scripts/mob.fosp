void _OnMessage(Critter&npc,Critter&fromCrit,int message,int value)
{
	if(npc.IsPlayer())
	return;
	if(not npc.IsLife())
	return;
	if(npc.Stat[(106)]==0)
	return;
	if(npc.Stat[(106)]==33)
	return;
	if(npc.Stat[(106)]!=fromCrit.Stat[(106)])
	return;
	
	if(fromCrit.IsPlayer())
	{
		AddAttackPlane(npc,10,fromCrit);
	}
	
	if(message==(16001))
	{
		Critter@target=::GetCritter(value);
		if(not(@target!=null))
		return;
		if(::GetCrittersDistantion(npc,fromCrit)>(35))
		return;
		AddAttackPlane(npc,0,target);
	}
	else if(message==(16002))
	{
		npc.StatBase[(97)]++;
	}
}

