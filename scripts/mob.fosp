}

void _BossIdle(Critter&npc)
{
	Map@map=npc.GetMap();
	if(map.GetData(0)<=0)return;
	
	if(Random(0,9)==0)
	{
		uint16 HomeHx=npc.HexX+Random(-20,20);
		uint16 HomeHy=npc.HexY+Random(-20,20);
		if(not map.IsHexPassed(HomeHx,HomeHy)||map.GetPathLength(npc,HomeHx,HomeHy,0)>50||GetDistantion(HomeHx,HomeHy,npc.HexX,npc.HexY)<=(15))return;
		
		npc.SetHomePos(HomeHx,HomeHy,Random(0,5));
		TryGoHome(npc);
		
		if(npc.StatBase[(92)]!=0)
		{
			uint[]minions={npc.StatBase[(92)],npc.StatBase[(93)],npc.StatBase[(94)],npc.StatBase[(95)]};
			for(uint i=0,ii=minions.length();i<ii;i++)
			{
				if(minions[i]==0||!(@GetCritter(5000000+minions[i])!=null))
				{
					if(i<ii-1)
					{
						minions[i]=minions[i+1];
						minions[i+1]=0;
					}
					else minions[i]=0;
				}
				if(minions[i]==0)continue;
				Critter@minion=GetCritter(5000000+minions[i]);
				if(!(@minion!=null)||minion.IsKnockout()||minion.IsDead())continue;
				else
				{
					if(5000000+minion.StatBase[(91)]!=npc.Id)
					{
						if(i<ii-1)
						{
							minions[i]=minions[i+1];
							minions[i+1]=0;
						}
						else minions[i]=0;
					}
					else
					{
						minion.SetHomePos(HomeHx+Random(-5-i,5+i),HomeHy+Random(-5-i,5+i),Random(0,5));
						TryGoHome(minion);
					}
				}
			}
			npc.StatBase[(92)]=minions[0];
			npc.StatBase[(93)]=minions[1];
			npc.StatBase[(94)]=minions[2];
			npc.StatBase[(95)]=minions[3];
		}
	}
}   

int _PlaneBegin(Critter&npc,NpcPlane&plane,int reason,Critter@someCr,Item@someItem)
{
	if(plane.Type==(1))
	{
		Critter@target=::GetCritter(plane.Attack_TargId);
		if(not(@target!=null))
		return(0);
		
		if(target.Stat[(106)]==npc.Stat[(106)])
		return(2);
		
		if(reason==(11))
		{
			uint distantion=::GetCrittersDistantion(npc,target);
			if(distantion<=(10))
			{ 
				
				uint pathLength=npc.GetMap().GetPathLength(npc,target.HexX,target.HexY,1);
				
				if(pathLength==0)
				return(2);
				
				if(pathLength>distantion*3)
				return(2);
				
				npc.SendMessage((16001),target.Id,(0));
				return(1);
			}
			else
			{
				
				return(2);
			}
		}
	}
	return(0);
}

