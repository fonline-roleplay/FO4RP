bool gm_msg( string& message )
{
    if( message.length() < 2 || message[ 1 ] == ' ' )
        return false;
    string@[] @ parts = split( message, " " );

    if( !valid( parts ) )
        return false;
    Command@ @command = GetCommandByName( parts[ 0 ] );

    if( !valid( command ) )  // no such command exists
    {
        return false;
    }
    for( uint i = 1; i < parts.length(); ++i )
		command.pushBackArg( CommandArgument( parts[i] ) );
    // {
        // CommandArgument arg;
        // int             number = 0;
        // bool            isNum = StrToInt( parts[ i ], number );
        // if( isNum )
        // {
            // arg = CommandArgument( number );
        // }
        // else
        // {
            // arg = CommandArgument( parts[ i ] );
        // }
        // command.pushBackArg( arg );
    // }

    command.run();
    string@ reformedMessage = command.getReformedMessage();
    if( valid( reformedMessage ) )  // message should be passed further
    {
        message = reformedMessage;
        return true;
    }
    return false;
}

// -- By Sacred Cracker
//Отключено, т.к. регулярно выбрасывало в меню карманных ГМов, ведь они не знают пароля от ГМки. Да и при перезапуске сервера каждый раз вводить пароль от ГМки - надоедает даже обычным ГМам.
//Именно поэтому на стороне СЕРВЕРА совершается проверка isGM(cr)- она проверяет не только допуск, но и наличие cr.Id в списке "карманных ГМов", настраиваемом прямо в процессе игры через файл: pocket_GMs.txt
bool RunGMScript( string@ script, int param0, int param1, int param2, string@ param3, int[]@ param4 )
{
	CritterCl@ chosen = GetChosen();
	if( @chosen !is null && chosen.Stat[ ST_ACCESS_LEVEL ] >= ACCESS_MODER )
	{
		RunServerScript( script, param0, param1, param2, param3, param4 );
		return true;
	}
	
	return false;
}



