class RunAdminFunction : Command
{
    RunAdminFunction()
    {
		argsRequired = 2;
		@description = "#run module function p0 p1 p2";
    }

    private void process()
    {
		string module = getArg(0).getStringData();
		string function = getArg(1).getStringData();
		
		CommandArgument@ arg0 = getArg(2);
		int p0 = valid( arg0 ) ? arg0.getIntData() : 0;

		CommandArgument@ arg1 = getArg(3);
		int p1 = valid( arg1 ) ? arg1.getIntData() : 0;

		CommandArgument@ arg2 = getArg(4);
		int p2 = valid( arg2 ) ? arg2.getIntData() : 0;
		
		//TODO: Хранить всю строку в string (для вариантов вызова "а-ля лексема" и т.п.) и список всех int[] аргументов (а не только первые три).

        int[] controlled = control();
        controlled.insertLast( current() );
		
		RunServerScriptUnsafe( module + "@unsafe_" + function, p0, p1, p2, null, controlled );
    }
}


