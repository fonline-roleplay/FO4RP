class CopyCommand : Command
{
    CopyCommand()
    {
		argsRequired = 0;
    }
    
	private void process()
    {
        CritterCl@ chosen = GetChosen();
        if( valid( chosen ) )
        {
			uint16     x = __MouseX, y = __MouseY;
			
			bool noTarget = false;
			
			int targetId = 0;
			CritterCl@ target = GetMonitorCritter( x, y );
			if( !valid(target) || target.Id == chosen.Id ) 
			{	
				noTarget = true;
			}
			else
				targetId = target.Id;

			bool noItem = false;
			
			int itemId = 0;
			ItemCl@ item = GetMonitorItem( x, y );
			if( !valid(item) && chosen.Param[ CR_TARGET_CONTAINER_ID ] == 0 )
			{	
				noItem = true;
			}
			else
				itemId = valid( item ) ? item.Id : chosen.Param[ CR_TARGET_CONTAINER_ID ];
				
			
			if( noItem && noTarget )
			{
				Message( "Наведите курсор на цель для копирования: НПЦ - дублируется со статами и лутом под вашим контролем. Игрок - при включенном режиме игры 'тестер' копирует его статы и лут, выводит но не копирует его активные таймивенты. Итем - копируется со всем содержимым, но без тайимвентов.");
				return;
			}

			RunServerScriptUnsafe( "test@unsafe_Copy", targetId, itemId, 0, null, null );
        }
    }
}

