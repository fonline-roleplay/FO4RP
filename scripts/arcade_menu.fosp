bool _noPickUp(Critter&cr,int skill,Critter@onCritter,Item@onItem,Scenery@onScenery)
{
	if(skill==(-1)||skill==(-5)||skill==(-7)||skill==(210))
	return true;
	return false;
}                                                                                                                                                                                                                                              

void BreakBalls(uint16 crType)
{
	Map@map=GetMapByPid((101),0);
	if(@map==null)
	return;
	Critter@[]crs;
	map.GetCritters(0,(0x01)|(0x20),crs);
	uint length=crs.length();
	if(length==0)
	return;
	for(uint i=0;i<length;i++)
	{
		if(@crs[i]!=null&&crs[i].StatBase[(112)]==58&&crs[i].StatBase[(67)]==crType)
		{
			crs[i].ToDead(0,null);
			return;
		}
	}
}                                                                                                                                                                                                                                                                                                                                                                                                                            

