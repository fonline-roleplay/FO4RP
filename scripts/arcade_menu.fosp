bool GetArcadeData(int[]&arcadeHive)
{
	
	if(IsAnyData("ARCADE_HIVE"))
	{
		GetAnyData("ARCADE_HIVE",arcadeHive);
	}
	else
	return false;
	
	return true;
}

void arcadehiveinsert(Critter&cr,uint16 x,uint16 y,uint8 type)
{
	int[]arcadeHive;   
	
	if(!GetArcadeData(arcadeHive))
	{
		cr.Say((11),"нет логов, создаем новым массив.");
	}
	uint data=(int(x)&0x3FF)|((int(y)&0x3FF)<<10)|((int(type)&0xFFF)<<20);
	arcadeHive.insertLast(data);
	if(SetAnyData("ARCADE_HIVE",arcadeHive))
	cr.Say((11),"добавлен логово No "+arcadeHive.length()+" x "+x+" y "+y+" тип "+type);
	else
	cr.Say((11),"ошибка записи, массив не записан. данные не изменены.");
}

void arcadehivelist(Critter&cr)
{
	int[]arcadeHive;
	if(!GetArcadeData(arcadeHive))
	{
		cr.Say((11),"нет логов, операция невозможна. выход.");
		return;
	}
	for(uint8 i=0;i<arcadeHive.length();i++)
	{
		uint16 x=(arcadeHive[i])&0x3FF;
		uint16 y=(arcadeHive[i]>>10)&0x3FF;
		uint16 type=(arcadeHive[i]>>20)&0xFFF;
		
		cr.Say((11),"логово No "+i+" x "+x+" y "+y+" тип(по БТ) "+type);
	}
}

void arcadehivedelone(Critter&cr,uint8 index)
{
	int[]arcadeHive;
	if(!GetArcadeData(arcadeHive))
	{
		cr.Say((11),"нет логов, операция невозможна. выход.");
		return;
	}
	uint8 length=arcadeHive.length();
	if(index>length)
	{
		cr.Say((11),"индекс > длинны. выход");
		return;
	}
	arcadeHive.removeAt(index);
	if(SetAnyData("ARCADE_HIVE",arcadeHive))
	cr.Say((11),"удален индекс No "+index+" старая длинна "+length+" новая "+arcadeHive.length()+" массив сдвинут на один влево с удаление элемента.");
	else
	cr.Say((11),"ошибка записи, массив не записан. данные не изменены.");
	
}

void arcadehiveclear(Critter&cr)
{
	int[]arcadeHive;
	if(!GetArcadeData(arcadeHive))
	{
		cr.Say((11),"нет логов, операция невозможна. выход.");
		return;
	}
	EraseAnyData("ARCADE_HIVE");
	cr.Say((11),"Количество удаленных логов : "+arcadeHive.length()); 
	
} 

