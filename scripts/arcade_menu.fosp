void JumpToTarget(Critter&cr,Critter@targetCr,int cost)
{
	Map@map=targetCr.GetMap();
	uint16 x=0,y=0;
	map.GetEntireCoords(255,0,x,y);
	cr.TransitToMap(map.Id,x,y+Random(10,130),0);
	cr.StatBase[(121)]=0;
	uint8 dir=GetDirection(cr.HexX,cr.HexY,targetCr.HexX,targetCr.HexY);
	cr.ParamBase[(72)]=cr.ParamBase[(7)]+200;
	ChangeCritterSpeed(cr);
	string place;
	if(dir==0)
	place="северо-востоке";
	else if(dir==1)
	place="востоке";
	else if(dir==2)
	place="юго-востоке";
	else if(dir==3)
	place="юго-западе";
	else if(dir==4)
	place="западе";
	else
	place="северо-западе";
	cr.ToKnockout((((Random(0,1)==1))?(82):(83)),(((Random(0,1)==1))?(86):(87)),(((Random(0,1)==1))?(88):(89)),10,cr.HexX,cr.HexY);
	cr.Say((11),"Вы чуете жертву на "+place);
	
	if(targetCr.GetTimeEvents((13),null,null,null)==0)
	targetCr.AddTimeEvent("cte_arcade_danger",__FullSecond,(13),cost);
	else
	targetCr.ParamBase[(184)]+=cost;
	
	cr.AddTimeEvent("cte_arcade_smelltarget",__FullSecond+((5)*__TimeMultiplier),(13),targetCr.Id);
}

uint cte_arcade_danger(Critter&cr,int identifier,uint&rate)
{
	if(cr.ParamBase[(184)]==0)
	cr.ParamBase[(184)]=rate;
	if(cr.ParamBase[(184)]>0)
	cr.ParamBase[(184)]--;
	if(cr.ParamBase[(184)]<=0)
	return 0;
	return(((20)*__TimeMultiplier));
}

uint cte_arcade_smelltarget(Critter&cr,int identifier,uint&rate)
{
	Critter@targetCr=GetCritter(cr.ParamBase[(188)]);
	if(!(@targetCr!=null))
	{
		cr.Say((11),"Вы потеряли след");
		cr.ToDead(0,null);
		return 0;
	}
	if(cr.StatBase[(121)]==1||cr.IsDead())
	{
		cr.Say((11),"Вы мертвы");
		return 0;
	}
	Map@map=cr.GetMap();
	Map@mapTg=targetCr.GetMap();
	if(!(@mapTg!=null))
	{
		cr.Say((11),"Вы потеряли след");
		cr.ToDead(0,null);
		return 0;
	}
	string place="";
	if(map.Id!=mapTg.Id)
	{
		Location@loc=map.GetLocation();
		Location@locTg=mapTg.GetLocation();
		if((loc.WorldX-locTg.WorldX)>20||(loc.WorldY-locTg.WorldY)>20)
		cr.ToDead(0,null);
		if(loc.WorldX<locTg.WorldX)
		place+="восточнее";
		else if(loc.WorldX>locTg.WorldX)
		place+="западнее";
		if(loc.WorldY>locTg.WorldY)
		place+=" южнее";
		else if(loc.WorldY<locTg.WorldY)
		place+=" севернее";
		cr.Say((11),"Вы чуете цель далеко "+place);
	}
	else
	{
		uint8 dir=GetDirection(cr.HexX,cr.HexY,targetCr.HexX,targetCr.HexY);
		if(dir==0)
		place="северо-востоке";
		else if(dir==1)
		place="востоке";
		else if(dir==2)
		place="юго-востоке";
		else if(dir==3)
		place="юго-западе";
		else if(dir==4)
		place="западе";
		else
		place="северо-западе";
		cr.Say((11),"Вы чуете жертву на "+place);
	}
	return(((5)*__TimeMultiplier));
} 

