void StartArcadeMenu(Critter@cr)
{
	cr.ParamBase[(703)]=(3);
	cr.StatBase[(121)]=1;
	Critter@[]crs;
	GetAllPlayers(crs);
	bool thereistarget=false;
	uint j=0,max=crs.length();
	if(max<2)
	return;
	
	int BonusWild=0;
	uint16 time=0;
	uint16 hour=0;
	GetTime(time,time,time,time,hour,time,time,time);
	if(7>hour&&hour>22)
	BonusWild=20;
	
	for(uint i=0;i<max;i++)
	{
		j=Random(0,max-1);
		if(not(@crs[j]!=null))
		continue;
		Map@map=crs[j].GetMap();
		if(not(@map!=null))
