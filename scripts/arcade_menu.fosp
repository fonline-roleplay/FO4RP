					cr.Say((19+(11)),"25 малый коготь");
				}
			}
			if(Wild>=50)
			{
				if(Wild>=100)
				cr.ShowScreen((2),9,"answer_StartArcadeMenu_2");
				else if(Wild>=70)
				cr.ShowScreen((2),8,"answer_StartArcadeMenu_2");
				else if(Wild>=60)
				cr.ShowScreen((2),6,"answer_StartArcadeMenu_2");
				else
				cr.ShowScreen((2),4,"answer_StartArcadeMenu_2");
				cr.Say((18),"Цель в диких землях, доступно "+delta+" едениц агрессии");
				cr.Say((19+(0)),"20 волк");
				cr.Say((19+(1)),"20 большой скорп");
