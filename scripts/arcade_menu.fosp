				else
				cr.ShowScreen((2),4,"answer_StartArcadeMenu_1");
				cr.Say((18),"Цель в городских окраинах, доступно "+delta+" едениц агрессии");
				cr.Say((19+(0)),"2 Крыса");
				cr.Say((19+(1)),"2 Мантис");
				cr.Say((19+(2)),"2 муравей");
				cr.Say((19+(3)),"4 м. скорпион");
				if(Wild>=20)
				{
					cr.Say((19+(4)),"7 свинокрыс");
					cr.Say((19+(5)),"10 собака");
					cr.Say((19+(6)),"10 гекко");
				}
				if(Wild>=30)
				{
					cr.Say((19+(7)),"12 кротокрыс");
					cr.Say((19+(8)),"15 большой скорпион");
					cr.Say((19+(9)),"15 золотой гекко");
