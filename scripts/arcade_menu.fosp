void unsafe_arcade(Critter&player,int param0,int param1,int param2,string@param3,int[]@param4)
{
	if(player.Param[(703)]!=(0))
	{
		player.Say((11),"ошибка, обратитесь к Мастерам");
		return;
	}
	if(player.Stat[(77)]>1||player.Stat[(76)]>0||player.Stat[(183)]>0)
	{
		player.SayMsg((11),(3),106941);
		return;
	}
	
	Item@[]items;
	player.GetItems((0),items);
	player.GetItems((1),items);
	player.GetItems((2),items);
	DeleteItems(items);
	DontPickItUp(player);
	
	if(player.Param[(703)]==(3))
	{
		player.SayMsg((11),(3),106940);
		return;
	}
	else
	{   
		
		player.ParamBase[(703)]=(3);
		Map@map=GetMapByPid(94,0);
		if(!(@map!=null))
		{
			player.Say((11),"Нет карты аркады, обратитесь к Мастерам.");
			return;
		}
		player.Say((11),"Включен аркадный режим игры.");
		player.TransitToMap(map.Id,0);
	}
	return;
} 

