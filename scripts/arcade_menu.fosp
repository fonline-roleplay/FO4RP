		Wild=(((Wild)>(Wild2))?(Wild2):(((Wild)<(0))?(0):(Wild))); 
		
		int level=cr.Stat[(77)];
		int danger=crs[j].ParamBase[(184)];
		
		if((Wild+BonusWild)<danger)
