void _TraderInit(Critter&trader,bool firstTime)
{ 
	
	int traderIndex=GetTraderIndex(trader);
	if(traderIndex>=0)
	{
		trader.StatBase[(115)]=5;
		trader.StatBase[(84)]=30;  
		
		trader.SkillBase[(215)]=50;
		trader.ModeBase[(520)]=1;
		trader.ModeBase[(512)]=0;
		trader.ModeBase[(523)]=1;
		trader.ModeBase[(511)]=1;
		trader.ModeBase[(532)]=1;
		trader.ModeBase[(513)]=1;
		
		if(trader.GetTimeEvents((3),null,null,null)==0)
		{
			uint nextCall=UpdateItems(trader);
			if(nextCall>0)
			trader.AddTimeEvent("cte_UpdateItems",nextCall,(3));
		}
	}
}

uint cte_UpdateItems(Critter&trader,int identifier,uint&stage)
{
	if(trader.IsDead())
	return trader.Timeout[(241)]+((1)*__TimeMultiplier*60);
	return UpdateItems(trader);
}

int GetTraderIndex(Critter&trader)
{
	
	int dialogId=trader.Stat[(104)];
	if(dialogId>0)
	return dialogId;
	return-1;        
	
}

uint UpdateItems(Critter&trader)
{
	int traderIndex=GetTraderIndex(trader);
	if(traderIndex<0)
	return 0; 
	
	Item@[]items;
	uint count=trader.GetItems((0),items);
	if(count!=0)
	{
		DeleteItems(items);
		return((2)*__TimeMultiplier);
	} 
	
	GenerateItemsForTrader(trader,traderIndex);                  
	
	return((2)*__TimeMultiplier*3600);
}
