uint GenerateItemsByCategory(Critter&trader,uint[]category)
{     
	
	uint deliveryQuality=GetDeliveryQuality();   
	
	uint quantity=category[0];
	uint quantityLow=1,quantityHigh=1;
	
	switch(quantity)
	{
		case(0):
		quantityLow=0;
		quantityHigh=1;
		break;
		case(1):
		quantityLow=0;
		quantityHigh=2;
		break;
		case(2):
		quantityLow=1;
		quantityHigh=5;
		break;
		case(3):
		quantityLow=3;
		quantityHigh=7;
		break;
		case(4):
		quantityLow=7;
		quantityHigh=15;
		break;
		case(5):
		quantityLow=15;
		quantityHigh=25;
		break;
		case(6):
		quantityLow=25;
		quantityHigh=35;
		break;
		case(7):
		quantityLow=5;
		quantityHigh=15;
		break;
		default:
		quantityLow=1;
		quantityHigh=1;
		break;
	}
	
	uint j=category.length();
	
	for(uint i=1;i<j;i++)
	{
		uint addQuantity=Random(quantityLow,quantityHigh);
		if(addQuantity==0)
		{ 
			
		}
		else if(category[i]==(41))
		{
			
			trader.AddItem(category[i],Random(100,200));
		}
		else if(category[i]==(519))
		{
			
			trader.AddItem(category[i],Random(100,200));
		}
		else
		{ 
			
			trader.AddItem(category[i],addQuantity);
			if(Random(0,100)>70&&deliveryQuality>0&&category[0]!=(0))
			trader.AddItem(category[i],1);
			if(Random(0,100)>90&&deliveryQuality>1&&category[0]!=(0))
			trader.AddItem(category[i],1);
		}
		
	}
	return 0;
}   

