        player.Say( SAY_DIALOGBOX_BUTTON( 7 ), "в соплю" );
        player.Say( SAY_DIALOGBOX_BUTTON( 8 ), "пополам" );
        player.Say( SAY_DIALOGBOX_BUTTON( 9 ), "назад" );
