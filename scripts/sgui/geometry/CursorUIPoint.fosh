final class CursorUIPoint : UIPointContract
{
    CursorUIPoint() 
    {}

    int GetScreenX() 
    {
        #ifdef __CLIENT
        return __MouseX;
        #endif
        #ifdef __SERVER
        return 0;
        #endif
    }

    int GetScreenY() 
    {
        #ifdef __CLIENT
        return __MouseX;
        #endif
        #ifdef __SERVER
        return 0;
        #endif
    }

    bool IsValid() 
    {
        return true;
    }

    void Refresh()
    {}
}