final class StaticUIPoint : UIPointContract
{
    int X;
    int Y;

    StaticUIPoint(int x, int y) 
    {
        this.X = x;
        this.Y = y;
    }

    int GetScreenX() 
    {
        return this.X;
    }
    
    int GetScreenY() 
    {
        return this.Y;
    }

    bool IsValid() 
    {
        return true;
    }

    void Refresh()
    {}
}