    if( cr.Param[ ST_PARALYSIS_LEVEL ] != 0 )
	{
        DropParalysisInstant( cr );
	}

	if( cr.Param[ ST_POISONING_LEVEL ] != 0 )
	{
        DropPoison( cr );
