#ifndef CONSUMABLES_DATA_DRUG_GROUPS
#define CONSUMABLES_DATA_DRUG_GROUPS

void init_consumables() //exported
{
	init_drug_groups();
	
	init_data();
}

DrugGroup@ dg_doping, dg_combat_drugs, dg_heavy, dg_healing_items, dg_therapy;
void init_drug_groups()
{
	if( !valid( dg_doping ) )
	{
		@dg_doping = DrugGroup( "DOPING" ).addPhase( 200 ).addPhase( 200 )
			.onOverdose( Effect()
				.add( AffectPoisonOnce( +40 ) )
			)
			.set( Addiction( ADDICTION_JET, 80 )
				.on( Effect( 2400 )
					.temp( ST_ACTION_POINTS_EXT, -10 )
					.temp( ST_STRENGTH_EXT, -1 )
					.temp( ST_AGILITY_EXT, -1 )
				)
			);
	}

	if( !valid( dg_combat_drugs ) )
	{
		@dg_combat_drugs = DrugGroup( "COMBAT" ).addPhase( 10 ).addPhase( 60 )
			.onOverdose( Effect()
				.add( Event_Knockout( 200 ) ) 
				.perm( TO_TIREDNESS, +200 )
				.add( Event_Heart_Attack( 5 ) )
			)
			.set( Addiction( ADDICTION_PSYCHO, 60 )
				.on( Effect( 1200 )
					.temp( ST_CHARISMA_EXT, -4 )
					.temp( ST_ENDURANCE_EXT, -2 )
					.temp( ST_MAX_LIFE_EXT, -30 )
					.temp( ST_APREGEN_EXT, -100 ) // multiplier is 100
				)
			);
	}
	
	if( !valid( dg_heavy ) )
	{
		@dg_heavy = DrugGroup( "HEAVY" ).addPhase( 600 ).addPhase( 60 )
			.onOverdose( Effect()
				.add( Event_Inflict_Injury() ) 
				.add( AffectPoisonOnce( +20 ) )
				.perm( ST_CURRENT_HP, -30 ) 
			)
			.set( Addiction( ADDICTION_BUFFOUT, 50 )
				.on( Effect( 2880 )
					.temp( ST_STRENGTH_EXT, -2 )
					.temp( ST_PERCEPTION_EXT, -3 )
					.temp( ST_CHARISMA_EXT, -1 )
					.temp( ST_INTELLECT_EXT, -1 )
					.temp( ST_AGILITY_EXT, -1 )
					.temp( ST_LUCK_EXT, -2 )
				)
			);		
	}
	
	if( !valid( dg_healing_items ) )
	{
		@dg_healing_items = DrugGroup( "HEALING" ).addPhase( 30 ).addPhase( 30 )
			.onOverdose( Effect()
				.add( Event_Bleed( +60 ) )
			);
	}

	if( !valid( dg_therapy ) )
	{
		@dg_therapy = DrugGroup( "THERAPY" ).addPhase( 10 ).addPhase( 20 )
			.onOverdose( Effect()
				.add( AffectPoisonOnce( +80 ) )
			)
			.set( Addiction( ADDICTION_MENTATS, 20 )
				.on( Effect( 2880 )
					.temp( ST_HEALING_RATE_EXT, -4 )
				)
			);
	}
}


#endif // CONSUMABLES_DATA_DRUG_GROUPS

